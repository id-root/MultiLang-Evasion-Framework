int my_stricmp(const char *a, const char *b) {
    while (*a && *b) {
        char ca = *a;
        char cb = *b;
        if (ca >= 'A' && ca <= 'Z') ca += 32;
        if (cb >= 'A' && cb <= 'Z') cb += 32;
        if (ca != cb) return ca - cb;
        a++; b++;
    }
    return 0;
}

DWORD GetPIDByName(const char* procName) {
    HMODULE hKernel32 = get_module_by_hash(H_KERNEL32);
    P_CreateToolhelp32Snapshot pCreateToolhelp32Snapshot = (P_CreateToolhelp32Snapshot)get_func_by_hash(hKernel32, H_CreateToolhelp32Snapshot);
    P_Process32First pProcess32First = (P_Process32First)get_func_by_hash(hKernel32, H_Process32First);
    P_Process32Next pProcess32Next = (P_Process32Next)get_func_by_hash(hKernel32, H_Process32Next);
    P_CloseHandle pCloseHandle = (P_CloseHandle)get_func_by_hash(hKernel32, H_CloseHandle);

    if (!pCreateToolhelp32Snapshot) return 0;

    HANDLE hSnap = pCreateToolhelp32Snapshot(TH32CS_SNAPPROCESS, 0);
    if (hSnap == INVALID_HANDLE_VALUE) return 0;

    PROCESSENTRY32 pe;
    pe.dwSize = sizeof(PROCESSENTRY32);

    if (pProcess32First(hSnap, &pe)) {
        do {
            if (my_stricmp(pe.szExeFile, procName) == 0) {
                pCloseHandle(hSnap);
                return pe.th32ProcessID;
            }
        } while (pProcess32Next(hSnap, &pe));
    }
    pCloseHandle(hSnap);
    return 0;
}

